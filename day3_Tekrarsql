

create table tedarikciler2 (
    vergi_no NUMBER(3) PRIMARY KEY,
    firma_ismi VARCHAR2(50),
    irtibat_ismi VARCHAR2(50)
    
    );
    
    INSERT INTO tedarikciler2 VALUES (101, 'IBM', 'Kim Yon');
