     create table urunler3(
     ted_vergino NUMBER(3), 
    urun_id NUMBER(11), 
    urun_isim VARCHAR2(50), 
    musteri_isim VARCHAR2(50),
    CONSTRAINT urunler3_fk FOREIGN KEY ( ted_vergino) REFERENCES tedarikciler2 (vergi_no)
    );
    
    
